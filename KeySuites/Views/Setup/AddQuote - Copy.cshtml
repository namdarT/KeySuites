                                            @Html.ValidationMessageFor(m => m.property.State)
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @Html.Label("Zip", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.property.Zip, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.property.Zip)
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @Html.Label("Floor", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.property.Floor, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.property.Floor)
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @Html.Label("Building Number", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.property.Building, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.property.Building)
                                        </div>
                                    </div>
                                </div>
                                <hr />
                                <h4 class="card-title text-info">Lease Schedule</h4>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @*Change asked by client Lease to Reservation*@
                                            @Html.Label("Reservation Start Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.LeaseStartDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @min = "1997-01-01", @max = "3030-31-12", @style = "border-radius:10px", @onchange = "startDateChange(event);", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.LeaseStartDate)

                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @*Change asked by client Lease to Reservation*@
                                            @Html.Label("Reservation End Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.LeaseEndDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @min = "1997-01-01", @max = "3030-31-12", @style = "border-radius:10px", @onchange = "endDateChange(event);", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.LeaseEndDate)

                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @Html.Label("Property Start Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @*@Html.TextBoxFor(m => m.PropertyStartDate, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })*@
                                            @Html.TextBoxFor(m => m.PropertyStartDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.PropertyStartDate)
                                        </div>
                                    </div>
                                    <div class="col-md-3" style="display:none">
                                        <div class="form-group label-floating">
                                            @Html.Label("Client Start Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @*@Html.TextBoxFor(m => m.ClientStartDate, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })*@
                                            @Html.TextBoxFor(m => m.ClientStartDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.ClientStartDate)
