    <script type="text/javascript">

        function endDateChange(e) {
            //alert("hi");
            var endDate = new Date(e.target.value);
            var startDate = new Date(document.getElementById('LeaseStartDate').value);
            //alert(endDate);
            var totalStay = endDate - startDate;
            totalStay = totalStay / (1000 * 60 * 60 * 24);
            document.getElementById('TotalStay').value = totalStay;
            cal();
        }
        function startDateChange(e) {
            //alert("hi");
            var startDate = new Date(e.target.value);
            var endDate = new Date(document.getElementById('LeaseEndDate').value);
            //alert(endDate);
            var totalStay = endDate - startDate;
            totalStay = totalStay / (1000 * 60 * 60 * 24);
            document.getElementById('TotalStay').value = totalStay;
            cal();
        }
        $(function () {
            $("#LeaseStartDate").onchange(function () {
                //alert("hi");
                var startDate = document.getElementById('LeaseStartDate').value;
                var endDate = document.getElementById('LeaseEndDate').value;
                var totalStay = document.getElementById('LeaseEndDate').value - document.getElementById('LeaseStartDate').value;
                document.getElementById('TotalStay').value = totalStay;
            });
        });
    </script>

