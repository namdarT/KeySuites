                                            @Html.Label("Property End Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @*@Html.TextBoxFor(m => m.PropertyEndDate, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })*@
                                            @Html.TextBoxFor(m => m.PropertyEndDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.PropertyEndDate)
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group label-floating">
                                            @Html.Label("Total Stay", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.TotalStay, new { @class = "form-control border-dark", @onchange = "mychange();", @type = "number", @value = "0", @step = "1", @min = "30", @max = "1000", @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.TotalStay)
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group label-floating" style="display:none">
                                            @Html.Label("Client End Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @*@Html.TextBoxFor(m => m.ClientEndDate, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })*@
                                            @Html.TextBoxFor(m => m.ClientEndDate, "{0:yyyy-MM-dd}", new { @class = "form-control border-dark", @type = "date", @style = "border-radius:10px", @required = "required" })
                                            @Html.ValidationMessageFor(m => m.ClientEndDate)
                                        </div>
                                    </div>

                                </div>
                                <hr />
