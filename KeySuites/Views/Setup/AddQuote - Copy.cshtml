                                </div>
                            </div>
                            <div class="col-md-3" style="display:none">
                                <div class="form-group label-floating">
                                    @Html.Label("Lead Id", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                    @Html.TextBoxFor(m => m.LeadsId, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                    @Html.ValidationMessageFor(m => m.LeadsId)
                                    @Html.Hidden("ModelLeadsId", Model.LeadsId)
                                    

                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group label-floating">
                                    @Html.Label("Company Name", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                    @Html.TextBoxFor(m => m.lead.LeadsName, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                    @Html.ValidationMessageFor(m => m.lead.LeadsName)
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group label-floating">
                                    @Html.Label("Occupant", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                    @Html.TextBoxFor(m => m.lead.OcupantName, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                    @Html.ValidationMessageFor(m => m.lead.OcupantName)
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group label-floating">
                                    @Html.Label("Prefered Address", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                    @Html.TextBoxFor(m => m.lead.PreferedAddress, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                    @Html.ValidationMessageFor(m => m.lead.PreferedAddress)
                                </div>
                            </div>
                            <div class="col-md-2" style="display:none">
                                <div class="form-group label-floating">
                                    @Html.Label("MoveIn Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                    @Html.TextBoxFor(m => m.lead.MoveInDate, new { @class = "form-control border-dark", @type = "varchar", @readonly = true, @style = "border-radius:10px", @required = "required" })
                                    @Html.ValidationMessageFor(m => m.lead.MoveInDate)
                                </div>
                            </div>
