            </div>*@
        <div class="row" id="table">
            <div class="col-md-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card">
                        <div class="card-body">

                            <h4 class="card-title text-info">Lead Info</h4>

                            @using (Html.BeginForm("AddQuote", "Setup", FormMethod.Post, new { enctype = "multipart/form-data" }))
