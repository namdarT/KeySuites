                                                        <div class="my-auto">

                                                            <a href="/Setup/DeleteProperty/@item.PropertyId" class="btnLink" data-toggle="tooltip" title="Delete"><i class="mdi mdi-delete "></i></a>

                                                        </div>
                                                    }
                                                    @if (ViewData["ProcessQuote"] == "p")
                                                    {
                                                        <div class="my-auto">

                                                            <a href="/Setup/AddQuote/@ViewData["LeadId"]?PropertyId=@item.PropertyId" class="btnLink" data-toggle="tooltip" title="Select Property"><i class="mdi mdi-account-arrow-right-outline "></i></a>


                                                        </div>

                                                    }
                                                </div>

                                            </td>



                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>

<script>



    function eventFired(type) {
        let n = document.querySelector('#demo_infoComp');
        n.innerHTML +=
            '<div>' + type + ' event - ' + new Date().getTime() + '</div>';
        n.scrollTop = n.scrollHeight;
    }

    new DataTable('#Comp')
        .on('order.dt', () => eventFired('Order'))
        .on('search.dt', () => eventFired('Search'))
        .on('page.dt', () => eventFired('Page'));

    new DataTable('#Comp').column(0).visible(true);
    new DataTable('#Comp').column(1).visible(true);

    new DataTable('#Comp').column(2).visible(true);
    new DataTable('#Comp').column(3).visible(false);
    new DataTable('#Comp').column(4).visible(false);
    new DataTable('#Comp').column(5).visible(false);
    new DataTable('#Comp').column(6).visible(false);
    new DataTable('#Comp').column(7).visible(false);
    new DataTable('#Comp').column(8).visible(false);
    new DataTable('#Comp').column(9).visible(false);
    new DataTable('#Comp').column(10).visible(false);
    new DataTable('#Comp').column(11).visible(false);
    new DataTable('#Comp').column(12).visible(false);
    //new DataTable('#Comp').column(13).visible(false);


    $("#btnshow").click(function () {
        new DataTable('#Comp').draw();
        new DataTable('#Comp').column(0).visible(false);
        new DataTable('#Comp').column(1).visible(false);
        new DataTable('#Comp').column(2).visible(false);
        new DataTable('#Comp').column(3).visible(true);
        new DataTable('#Comp').column(4).visible(true);
        new DataTable('#Comp').column(5).visible(true);
        new DataTable('#Comp').column(6).visible(true);
        new DataTable('#Comp').column(7).visible(true);
        new DataTable('#Comp').column(8).visible(true);
        new DataTable('#Comp').column(9).visible(true);
        new DataTable('#Comp').column(10).visible(true);
        new DataTable('#Comp').column(11).visible(true);
        new DataTable('#Comp').column(12).visible(true);
        //new DataTable('#Comp').column(13).visible(true);
        //table.column(12).visible(true);
        $("#ShowImage").hide(true);
        $("#Back").show(true);
    });

    $("#btnhide").click(function () {
        //var table = $('#Comp').DataTable();
        new DataTable('#Comp').column(0).visible(true);
        new DataTable('#Comp').column(1).visible(true);
        new DataTable('#Comp').column(2).visible(true);
        new DataTable('#Comp').column(3).visible(false);
        new DataTable('#Comp').column(4).visible(false);
        new DataTable('#Comp').column(5).visible(false);
        new DataTable('#Comp').column(6).visible(false);
        new DataTable('#Comp').column(7).visible(false);
        new DataTable('#Comp').column(8).visible(false);
        new DataTable('#Comp').column(9).visible(false);
        new DataTable('#Comp').column(10).visible(false);
        new DataTable('#Comp').column(11).visible(false);
        new DataTable('#Comp').column(12).visible(false);
        //new DataTable('#Comp').column(13).visible(false);
        //table.column(12).visible(false);
        $("#Back").hide(true);
        $("#ShowImage").show(true);
    });



</script>


