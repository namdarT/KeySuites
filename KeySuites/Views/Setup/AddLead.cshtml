    <script type="text/javascript">
        $(document).ready(function () {
            if ($("#ContactType").val() == 'Company') {
                document.getElementById('CompanyInfo').style.display = 'block';
                document.getElementById('IndividualInfo').style.display = 'none';
                document.getElementById('OccupantInfo').style.display = 'block';
            }
            else if ($("#ContactType").val() == 'Individual') {
                document.getElementById('CompanyInfo').style.display = 'none';
                document.getElementById('IndividualInfo').style.display = 'block';
                document.getElementById('OccupantInfo').style.display = 'none';
            }

        });

        $(document).on('change', "#ContactType", function () {
            if ($(this).val() == 'Company') {
                document.getElementById('CompanyInfo').style.display = 'block';
                document.getElementById('IndividualInfo').style.display = 'none';
                document.getElementById('OccupantInfo').style.display = 'block';
            }
            else if ($(this).val() == 'Individual') {
                document.getElementById('CompanyInfo').style.display = 'none';
                document.getElementById('IndividualInfo').style.display = 'block';
                document.getElementById('OccupantInfo').style.display = 'none';
            }
        });

    </script>

