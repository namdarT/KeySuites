<script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>


<table id="unit">
    <tr>
        <td>


            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="page-header">
                        <h3 class="page-title">Units Status Report</h3>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">

                                <li class="breadcrumb-item active" aria-current="page"> Unit Status </li>
                            </ol>
                        </nav>
                    </div>
                    @if (ViewData["SuccessMessage"] != "" && ViewData["SuccessMessage"] != null)
                    {
                        <div id="mess" class="row" style="display:block">

                            <div class="alert alert-success" role="alert">
                                <strong>Alert!</strong><hr />
                                @Html.Label("" + ViewData["SuccessMessage"].ToString(), new { @style = "color:pink" })
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                        </div>

                    }
                    <div class="row">
                        <div class="col-lg-12 grid-margin stretch-card">
                            <div class="card">
                                <div class="card-body">
                                    @*<h4 class="card-title">Unit List</h4>
                                        <p class="card-description">
                                            Each Property <code>Status Report</code>
                                        </p>*@
                                    <div class="row">
                                        @using (Html.BeginForm("UnitStatus", "Report", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                        {

                                            <div class="col-md-6">
                                                <div class="form-group label-floating">
                                                    @Html.Label("From Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                                    @*@Html.EditorFor(m => m.First().ValetTrash, new { @class = "form-control border-dark", @type = "date", @style = "font-size:12px;color:black;border-radius:10px", @required = "required" })*@
                                                    @Html.TextBoxFor(m => m.First().CreatedBy, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group label-floating">
                                                    @Html.Label("To Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                                    @*@Html.EditorFor(m => m.First().BreakLeasePolicy, new { @class = "form-control border-dark", @type = "date", @style = "font-size:12px;color:black;border-radius:10px", @required = "required" })*@
                                                    @Html.TextBoxFor(m => m.First().CreatedDatee, "{0:s}", new { @class = "form-control border-dark", @type = "datetime-local", @style = "border-radius:10px", @required = "required" })
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                @Html.AntiForgeryToken()
                                                <button type="submit" class="btn btn-primary btn-rounded btn-fw">

                                                    Search
                                                </button>
                                            </div>
                                        }

                                        <div class="col-md-6">
                                            @using (Html.BeginForm("Export", "Report", FormMethod.Post))
                                            {
                                                <input type="hidden" name="GridHtml" />
                                                <input type="submit" id="btnSubmit" class="btn btn-primary btn-rounded btn-fw" value="Export to Excel" />
                                            }
                                        </div>
                                        <hr />

                                        <div class="col-md-6">

                                            @Html.Label("Selected From Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.First().CreatedBy, new { @class = "control-label", @readonly = "true", @style = "font-size:12px;color:black" })
                                        </div>
                                        <div class="col-md-6">
                                            @Html.Label("Selected To Date", new { @class = "control-label", @style = "font-size:12px;color:black" })
                                            @Html.TextBoxFor(m => m.First().CreatedDatee, new { @class = "control-label", @readonly = "true", @style = "font-size:12px;color:black" })
                                        </div>

                                    </div>
                                    <div class="row">

                                    </div>
                                    <div id="table" class="table-responsive">
                                        <div id="demo_infoComp" class="box" style="display:none"></div>
                                        <table id="Comp" class="display" style="font-size:12px;color:black">
                                            <thead>
                                                <tr>
                                                    @*<th width="10%" >
                                                            @Html.DisplayName("Attribute")
                                                        </th>*@

                                                    <th>Description</th>

                                                    <th>Address</th>
                                                    <th>City</th>
                                                    <th>Area</th>
                                                    <th>Floor</th>
                                                    <th>Admin Fee</th>
                                                    <th>Pet Fee </th>
                                                    <th style="display:none">Application Fee </th>
                                                    <th>Status </th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model)
                                                {
                                                    <tr class="table-striped">
                                                        @*<td>
                                                                <div class="d-flex px-2 border-end">
                                                                    <div class="my-auto">
                                                                        Rain Meter
                                                                    </div>
                                                                </div>

                                                            </td>*@
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    <a href="/Setup/AddProperty/@item.PropertyId">@Html.DisplayFor(modelItem => item.PropertyDescription)</a>

                                                                </div>
                                                            </div>

                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    @Html.DisplayFor(modelItem => item.PropertyAddress)

                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    @Html.DisplayFor(modelItem => item.City)

                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">


                                                                    @Html.DisplayFor(modelItem => item.Area)
                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">


                                                                    @Html.DisplayFor(modelItem => item.Floor)
                                                                </div>
                                                            </div>


                                                        </td>


                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    @Html.DisplayFor(modelItem => item.AdminFee)
                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    @Html.DisplayFor(modelItem => item.PetFee)
                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td style="display:none">
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto">

                                                                    @Html.DisplayFor(modelItem => item.ApplicationFee)
                                                                </div>
                                                            </div>


                                                        </td>
                                                        <td>
                                                            <div class="d-flex px-2 border-end">
                                                                <div class="my-auto badge-busy">

                                                                    <code><h6>@Html.DisplayFor(modelItem => item.Status)</h6> </code>
                                                                </div>
                                                            </div>


                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </td>
    </tr>
</table>
<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>


<script>



    function eventFired(type) {
        let n = document.querySelector('#demo_infoComp');
        n.innerHTML +=
            '<div>' + type + ' event - ' + new Date().getTime() + '</div>';
        n.scrollTop = n.scrollHeight;
    }

    new DataTable('#Comp')
        .on('order.dt', () => eventFired('Order'))
        .on('search.dt', () => eventFired('Search'))
        .on('page.dt', () => eventFired('Page'));



</script>


<script>
    $(document).ready(function () {
        $("#ValetTrash").datepicker();

    });
</script>
<script>
    $(document).ready(function () {
        $("#BreakLeasePolicy").datepicker();

    });
</script>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#btnSubmit").click(function () {
            $("input[name='GridHtml']").val($("#unit").html());
        });
    });
</script>
